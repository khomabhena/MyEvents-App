            android:layout_height="wrap_content" />

        <ImageView
            app:srcCompat="@drawable/profile_money"
            android:layout_marginStart="20dp"
            app:layout_constraintTop_toTopOf="@+id/etPrice1"
            app:layout_constraintBottom_toBottomOf="@+id/etPrice1"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="25dp"
            android:layout_height="25dp" />

        <EditText
            android:id="@+id/etPrice2"
            android:hint="Price (2nd)"
            android:textSize="16sp"
            android:textColor="#393939"
            android:textColorHint="#939393"
            android:fontFamily="serif"
            android:inputType="number"
            android:layout_marginTop="20dp"
            android:layout_marginStart="60dp"
            android:layout_marginEnd="30dp"
            app:layout_constraintTop_toBottomOf="@+id/etName2"
            app:layout_constraintRight_toRightOf="parent"

                    android:textSize="25sp"
                    android:textStyle="bold"
                    android:textAllCaps="true"
                    android:textColor="#008080"
                    android:fontFamily="serif"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    card_view:layout_constraintTop_toBottomOf="@+id/tvTicketS"
                    card_view:layout_constraintRight_toRightOf="parent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/tvVenueS"
                    android:hint="Cole's Cafe"
                    android:textSize="14sp"
                    android:textColor="#000000"
                    android:textAllCaps="true"
                    android:fontFamily="serif"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    card_view:layout_constraintTop_toBottomOf="@+id/tvTimeS"
                    card_view:layout_constraintLeft_toRightOf="@+id/viewS"
                    card_view:layout_constraintRight_toLeftOf="@+id/tvLocationS"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/tvLocationS"
                    android:hint="BULAWAY0"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:textColor="#000000"
                    android:textAllCaps="true"
                    android:fontFamily="serif"
                    android:layout_marginTop="9dp"
                    android:layout_marginEnd="10dp"
                    card_view:layout_constraintTop_toBottomOf="@+id/tvTimeS"
                    card_view:layout_constraintRight_toRightOf="parent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/tvNameS"
                    android:hint="Special Saturdays"
                    android:textSize="12sp"
                    android:textColor="#565656"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="10dp"
                    card_view:layout_constraintTop_toBottomOf="@+id/tvVenueS"
                    card_view:layout_constraintLeft_toRightOf="@+id/viewS"
                    card_view:layout_constraintRight_toRightOf="parent"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <LinearLayout
                    android:gravity="start"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="20dp"
                    card_view:layout_constraintTop_toBottomOf="@+id/tvNameS"
                    card_view:layout_constraintLeft_toRightOf="@+id/viewS"
                    card_view:layout_constraintRight_toLeftOf="@+id/tvPriceS"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="serif"
                        android:hint="Pay to: "
                        android:paddingBottom="5dp"
                        android:paddingTop="5dp"
                        android:textSize="10sp" />

                    <TextView
                        android:id="@+id/tvMerchantNameS"
                        android:hint="ECOCASH MERCHANT NAME"
                        android:textSize="12sp"
                        android:textColor="#008080"
                        android:textStyle="bold"
                        android:textAllCaps="true"
                        android:fontFamily="serif"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:paddingStart="8dp"
                        android:paddingEnd="8dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tvPriceS"
                    android:hint="$0"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:textAllCaps="true"
                    android:textColor="#000000"
                    android:fontFamily="serif"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="10dp"
                    card_view:layout_constraintTop_toBottomOf="@+id/tvNameS"
                    card_view:layout_constraintRight_toRightOf="parent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </android.support.constraint.ConstraintLayout>

        </android.support.v7.widget.CardView>

    </android.support.constraint.ConstraintLayout>

</ScrollView>

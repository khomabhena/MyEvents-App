                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="E"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="V"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="E"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="N"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="T"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="S"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="A"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="P"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="P"
                        android:textSize="10sp"
                        android:textColor="#1f444d"
                        android:fontFamily="serif"
